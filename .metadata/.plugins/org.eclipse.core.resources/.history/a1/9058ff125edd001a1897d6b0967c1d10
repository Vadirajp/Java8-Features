package ThreadDemo;

public class Demo02 extends Thread {

	public void start() {
		System.out.println("start");
	}
	
	public void run() {
		System.out.println("run");
	}
	
	public static void main(String[] args) {
		Demo02 t1 = new Demo02();
		Demo02 t2 = new Demo02();

		t1.start();
		t2.start();
		
	}

}
